{
    "config_class": "DQNAuxAgentConfig",
    "fixed_parameters": {
        "exp_name": "test/gridhard/online_property/dqn_aux/freq8/input_decoder/best",
        "env_name": "GridHardRGB",
        "agent": "DQNAuxAgent",
        "discount": 0.99,
        "state_norm_coef": 255.0,

        "epsilon": 0.1,

        "rep_config": {
            "rep_type": "online",
            "network_type": "conv",
            "conv_architecture": {
                "conv_layers": [
                   {"in": 3,  "out": 16, "kernel": 4, "stride": 2, "pad": 2},
                   {"in": 16,  "out": 16, "kernel": 4, "stride": 2, "pad": 2},
                   {"in": 16,  "out": 8, "kernel": 4, "stride": 2, "pad": 2}
                ]
            },
            "in_dim": [15, 15, 3],
            "out_dim": 32,
            "load_params": false
        },

        "val_fn_config": {
            "val_fn_type": "linear",
            "init_type": "lta"
        },

        "aux_config": [
            {"aux_fn_type":  "deconv", "aux_in_dim": 32, "aux_out_dim": [15, 15, 3], "aux_task": "input_decoder",
            "deconv_architecture": {
                "fc_layers": {
                    "hidden_units": [72]
                },
                "deconv_layers": [
                   {"in": 8,  "out": 16, "kernel": 4, "stride": 2, "pad": 2, "out_pad":1},
                   {"in": 16,  "out": 16, "kernel": 4, "stride": 2, "pad": 2, "out_pad":0},
                   {"in": 16,  "out": 3, "kernel": 4, "stride": 2, "pad": 2, "out_pad":1}
                ]
            }
            }
        ],

        "replay": true,
        "memory_size": 10000,
        "batch_size": 32,

        "optimizer_type": "Adam",
        "use_target_network": true,
        "target_network_update_freq": 8,

        "max_steps": 300000,
        "log_interval": 10000,
        "eval_interval": 10000,
        "eval_episodes": 5,
        "save_interval": 0,
        "timeout": 100,
        "stats_queue_size": 100,

        "tensorboard_logs": false,
        "visualize": false,
        "save_params": true,
        "save_early": {"mean": 1.0, "min": 1.0},

        "online_property": true,
        "evaluate_lipschitz": true,
        "evaluate_distance": true,
        "evaluate_orthogonality": true,
        "evaluate_interference": true,
        "evaluate_diversity": true,
        "evaluate_sparsity": true,
        "distance_paths": [{"current": "../dataset/gridhard/random_dqn/distance_current_states.npy", "action": "../dataset/gridhard/random_dqn/distance_actions.npy", "next": "../dataset/gridhard/random_dqn/distance_next_states.npy", "reward": "../dataset/gridhard/random_dqn/distance_rewards.npy", "terminal": "../dataset/gridhard/random_dqn/distance_terminals.npy"}]
    },
    "sweep_parameters": {
        "learning_rate": [0.0001]
    }
}
