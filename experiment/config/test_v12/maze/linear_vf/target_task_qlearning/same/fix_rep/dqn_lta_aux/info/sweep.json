{
    "config_class": "QLearningAgentConfig",
    "fixed_parameters": {
        "exp_name": "test_v12/maze/linear_vf/target_task_qlearning/same/fix_rep/dqn_lta_aux/info/sweep",
        "env_name": "GridHardRGBGoal",
        "agent": "QLearningAgent",
        "discount": 0.99,
        "state_norm_coef": 255.0,

        "decay_epsilon": false,
        "epsilon": 0.1,


        "optimizer_type": "Adam",

        "max_steps": 300000,
        "log_interval": 10000,
        "eval_interval": 10000,
        "eval_episodes": 5,
        "save_interval": 0,
        "timeout": 100,
        "stats_queue_size": 100,

        "rep_config": {
            "rep_type": "modular",
            "network_type": "conv",
            "conv_architecture": {
                "conv_layers": [
                   {"in": 3,  "out": 32, "kernel": 4, "stride": 1, "pad": 1},
                   {"in": 32,  "out": 16, "kernel": 4, "stride": 2, "pad": 2}
                ]
            },
            "in_dim": [15, 15, 3],
            "out_dim": 64,
            "train_rep": false,
            "load_params": true,
            "path": "test_v12/maze/linear_vf/source_task/dqn_lta_aux/info/best/{}_run/0_param_setting/parameters/rep_net_earlystop"
        },

        "activation_config": {
            "name": "LTA",
            "input": 64,
            "tile": 20,
            "eta": 0.2,
            "bound_high": 2,
            "bound_low": -2
        },

        "val_fn_config": {
            "val_fn_type": "linear",
            "init_type": "xavier"
        },

        "online_property": false,
        "distance_paths": [{"current": "../dataset/gridhard/random_dqn/distance_current_states.npy", "action": "../dataset/gridhard/random_dqn/distance_actions.npy", "next": "../dataset/gridhard/random_dqn/distance_next_states.npy", "reward": "../dataset/gridhard/random_dqn/distance_rewards.npy", "terminal": "../dataset/gridhard/random_dqn/distance_terminals.npy"}],
        "evaluate_interference": false,
        "evaluate_lipschitz": false,
        "lipschitz_sampled_states_path": "../dataset/gridhard/random_dqn/lip_sampled_states.npy"
    },

    "sweep_parameters": {
        "learning_rate": [0.0003, 0.0001, 0.00003, 0.00001, 0.000003],
        "goal_id": [0]
    }
}
