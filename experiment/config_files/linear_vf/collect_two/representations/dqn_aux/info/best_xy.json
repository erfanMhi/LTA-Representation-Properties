{
    "config_class": "DQNAuxAgentConfig",
    "fixed_parameters": {
        "exp_name": "linear_vf/collect_two/representations/dqn_aux/info/best_xy",
        "env_name": "CollectTwoColorLip",
        "agent": "DQNAuxAgent",
        "discount": 0.99,
        "state_norm_coef": 255.0,

        "epsilon": 0.1,

        "rep_config": {
            "rep_type": "online",
            "network_type": "conv",
            "conv_architecture": {
                "conv_layers": [
                   {"in": 3,  "out": 32, "kernel": 4, "stride": 1, "pad": 1},
                   {"in": 32,  "out": 32, "kernel": 4, "stride": 2, "pad": 2},
                   {"in": 32,  "out": 16, "kernel": 4, "stride": 2, "pad": 2}
                ]
            },
            "in_dim": [15, 15, 3],
            "out_dim": 32,
            "load_params": false,
            "train_rep": true
        },

        "val_fn_config": {
            "val_fn_type": "linear"
        },

        "aux_config": [
            {
            "aux_fn_type":  "fc",
            "aux_in_dim": 32,
            "aux_out_dim": 2,
            "hidden_units": [32, 32],
            "aux_task": "rgb2xy"
            }
        ],

        "replay": true,
        "memory_size": 100000,
        "batch_size": 32,

        "optimizer_type": "Adam",
        "use_target_network": true,
        "target_network_update_freq": 1024,

        "max_steps": 1000000,
        "log_interval": 10000,
        "eval_interval": 50000,
        "eval_episodes": 5,
        "save_interval": 0,
        "timeout": 500,
        "stats_queue_size": 100,

        "tensorboard_logs": false,
        "save_params": true,
        "visualize": false,

        "evaluate_lipschitz": false,
        "lipschitz_sampled_states_path": "linear_vf/collect_two/trained_dqn/lip_sampled_states.npy",
        "rewarding_color": "green"
    },
    "sweep_parameters": {
        "learning_rate": [0.00003]
    }
}
