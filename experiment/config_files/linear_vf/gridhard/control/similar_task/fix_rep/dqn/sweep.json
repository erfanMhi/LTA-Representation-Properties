{
    "config_class": "DQNAgentConfig",
    "fixed_parameters": {
        "exp_name": "linear_vf/gridhard/control/similar_task/fix_rep/dqn/sweep",
        "env_name": "GridHardRGBGoal",
        "agent": "DQNAgent",
        "discount": 0.99,
        "state_norm_coef": 255.0,

        "epsilon": 0.1,

        "replay": true,
        "memory_size": 10000,
        "batch_size": 32,

        "optimizer_type": "Adam",
        "use_target_network": true,
        "target_network_update_freq": 64,

        "max_steps": 150000,
        "log_interval": 10000,
        "eval_interval": 10000,
        "eval_episodes": 5,
        "save_interval": 0,
        "timeout": 100,
        "stats_queue_size": 100,

        "rep_config": {
            "rep_type": "online",
            "network_type": "conv",
            "conv_architecture": {
                "conv_layers": [
                   {"in": 3,  "out": 16, "kernel": 4, "stride": 2, "pad": 2},
                   {"in": 16,  "out": 16, "kernel": 4, "stride": 2, "pad": 2},
                   {"in": 16,  "out": 8, "kernel": 4, "stride": 2, "pad": 2}
                ]
            },
            "in_dim": [15, 15, 3],
            "out_dim": 32,
            "train_rep": false,
            "load_params": true,
            "path": "linear_vf/gridhard/representations/dqn/best/{}_run/0_param_setting/parameters/rep_net"
        },

        "val_fn_config": {
            "val_fn_type": "linear"
         },

        "evaluate_lipschitz": true,
        "lipschitz_sampled_states_path": "linear_vf/gridhard/random_dqn/lip_sampled_states.npy"
    },

    "sweep_parameters": {
        "learning_rate": [0.01, 0.001, 0.0001, 0.00001],
        "goal_id": [4]
    }
}
