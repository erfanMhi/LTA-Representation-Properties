{
    "config_class": "DQNAuxAgentConfig",
    "fixed_parameters": {
        "exp_name": "linear_vf/gridhard/representations/dqn_aux/aux_control/best_1g",
        "env_name": "GridHardRGB",
        "agent": "DQNAuxSuccessorAgent",
        "discount": 0.99,
        "state_norm_coef": 255.0,

        "epsilon": 0.1,

        "rep_config": {
            "rep_type": "online",
            "network_type": "conv",
            "conv_architecture": {
                "conv_layers": [
                   {"in": 3,  "out": 16, "kernel": 4, "stride": 2, "pad": 2},
                   {"in": 16,  "out": 16, "kernel": 4, "stride": 2, "pad": 2},
                   {"in": 16,  "out": 8, "kernel": 4, "stride": 2, "pad": 2}
                ]
            },
            "in_dim": [15, 15, 3],
            "out_dim": 32,
            "load_params": false
        },

        "val_fn_config": {
            "val_fn_type": "linear"
         },


        "aux_config": [
            {
