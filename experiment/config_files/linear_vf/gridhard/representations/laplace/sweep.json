{
    "config_class": "LaplaceConfig",
    "fixed_parameters": {
        "exp_name": "linear_vf/gridhard/representations/laplace/sweep",
        "env_name": "GridHardRGB",
        "agent": "Laplace",
        "discount": 0.99,
        "state_norm_coef": 255.0,

        "rep_config": {
            "rep_type": "modular",
            "network_type": "conv",
            "conv_architecture": {
                "conv_layers": [
                   {"in": 3,  "out": 16, "kernel": 4, "stride": 2, "pad": 2},
                   {"in": 16,  "out": 16, "kernel": 4, "stride": 2, "pad": 2},
                   {"in": 16,  "out": 8, "kernel": 4, "stride": 2, "pad": 2}
                ]
            },
            "in_dim": [15, 15, 3],
            "out_dim": 32,
            "load_params": false,
            "train_rep": true
        },

        "replay": true,
        "memory_size": 10000,
        "batch_size": 32,
        "lmbda": 0.9,

        "optimizer_type": "Adam",

        "max_steps": 30000,
        "log_interval": 1000,
        "eval_interval": 1000,
        "eval_episodes": 5,
        "save_interval": 0,
        "timeout": 50,
        "stats_queue_size": 100,

        "tensorboard_logs": false,
        "save_params": true,
        "visualize": true
    },
    "sweep_parameters": {
        "learning_rate": [0.001, 0.0003, 0.0001, 0.00003],
        "beta": [1, 2, 3],
        "delta": [0.2, 0.1, 0.05]
    }
}
